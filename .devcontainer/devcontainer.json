{
    "name": "My Dev Container",
    "dockerFile": "Dockerfile",
    "context": "..",
    "appPort": [3000],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-vscode.node-debug2",
        "dbaeumer.vscode-eslint"
    ],
    "postCreateCommand": "npm install",
    "remoteUser": "node",
    "runArgs": [
        "--network", "my_internal_network"
    ]
}
